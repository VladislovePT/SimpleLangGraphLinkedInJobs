# LangSmith Tracing (Optional)
LANGSMITH_API_KEY="your_langsmith_api_key"
LANGSMITH_TRACING_V2="true"
LANGSMITH_PROJECT="your_project_name"
LANGSMITH_ENDPOINT="https://api.smith.langchain.com"

# API Keys
TAVILY_API_KEY="your_tavily_api_key"
OPENAI_API_KEY="your_openai_api_key"

# Azure DevOps PAT
# Required for writing to the wiki. Must have Wiki (Read & write) permissions.
AZURE_DEVOPS_PAT="your_azure_devops_personal_access_token"

# LinkedIn MCP (Multi-Channel Proxy)
# The li_at cookie for your LinkedIn account.
LINKEDIN_COOKIE="your_linkedin_li_at_cookie"
# The URL where your LinkedIn MCP server is running.
LINKEDIN_MCP_URL="http://<your_mcp_server_ip>:<port>/mcp/"

# Script Configuration
MODEL_NAME="gpt-4o-mini"
TOOL_CONFIG_PATH="linkedin_agent/model/mcp-tool-names.json"
PROFILE_JSON_PATH="linkedin_agent/model/profile.json"

# Azure DevOps Wiki Configuration
ORGANIZATION="your_devops_organization"
PROJECT="your_devops_project"
WIKI_ID="your_project_wiki_name.wiki"
PAGE_PATH="/Jobs"

# Database Configuration
DB_HOST="mysql_host"
DB_USER="user"
DB_PASSWORD="password"
DB_NAME="database_name" 